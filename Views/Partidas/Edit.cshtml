@model WebApplication1.Entity.Partida

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Partida</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.PuntajeId)

        @Html.HiddenFor(model => model.UsuarioId)

        @Html.HiddenFor(model => model.Puntaje)

        @Html.HiddenFor(model => model.ColorFicha)

        @Html.HiddenFor(model => model.CantidadMovimientos)

        @Html.HiddenFor(model => model.EstadoPartida)

        @Html.HiddenFor(model => model.TipoPartida)

        @Html.HiddenFor(model => model.FechaPartida)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
