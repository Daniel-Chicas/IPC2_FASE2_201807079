                segundosTotales = 0;
                segundo = document.getElementById("segundos");
                minuto = document.getElementById("minutos");
                segundo.innerHTML = "00";
                minuto.innerHTML = "00";
                cronometro = setInterval(function () {
                    segundosTotales++;
                    secs = segundosTotales;
                    mins = 0;
                    while (secs >= 60) {
                        mins++;
                        secs -= 60;
                    }
                    if (mins < 10) minuto.innerHTML = "0" + mins;
                    else minuto.innerHTML = mins;
                    if (secs < 10) segundo.innerHTML = "0" + secs;
                    else segundo.innerHTML = secs;

                    totalSegundos = secs;
                    totalMinutos = mins;

